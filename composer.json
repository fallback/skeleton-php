{
    "name": "owenvoke/skeleton-php",
    "type": "library",
    "description": ":package_description",
    "license": "MIT",
    "require": {
        "php": "^7.4"
    },
    "require-dev": {
        "pestphp/pest": "^0.1.5",
        "psalm/plugin-laravel": "^1.3",
        "vimeo/psalm": "^3.11"
    },
    "autoload": {
        "psr-4": {
            "OwenVoke\\Skeleton\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OwenVoke\\Skeleton\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "pest",
        "analyse": "psalm"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
